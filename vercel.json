{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  // "routes": [
  //   {
  //     "src": "/(.*)",
  //     "dest": "/$1"
  //   }
  // ],
  "rewrites":[
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
